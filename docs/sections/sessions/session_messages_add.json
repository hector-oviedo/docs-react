  
 [
    {
      "type": "header",
      "h": 3,
      "content": "Add Message"
    },
    {
      "type": "p",
      "content": "Adds a new message to the session from either user or assistant."
    },
    {
      "type": "header",
      "h": 5,
      "content": "METHOD: POST"
    },
    {
      "type": "snippet",
      "language": "bash",
      "content": "/sessions/{session_id}/messages"
    },
    {
      "type": "header",
      "h": 3,
      "content": "Body"
    },
    {
      "type": "table",
      "headers": ["Field", "Type", "Required", "Description"],
      "rows": [
        ["sender", "string", "Yes", "\"user\" or \"assistant\"."],
        ["content", "string", "Yes", "Message text."],
        ["think", "string", "No", "Hidden 'thinking' text, if any."]
      ]
    },
    {
      "type": "header",
      "h": 3,
      "content": "Response"
    },
    {
      "type": "snippet",
      "language": "json",
      "content": "{\n  \"id\": 15,\n  \"sender\": \"user\",\n  \"content\": \"Hello again\",\n  \"think\": \"\"\n}"
    }
  ]