[
    {
      "type": "header",
      "h": 3,
      "content": "List Documents"
    },
    {
      "type": "p",
      "content": "Returns a list of all documents belonging to the current user, including session attachments."
    },
    {
      "type": "header",
      "h": 5,
      "content": "METHOD: GET"
    },
    {
      "type": "snippet",
      "language": "bash",
      "content": "/documents/list"
    },
    {
      "type": "header",
      "h": 3,
      "content": "Response"
    },
    {
      "type": "snippet",
      "language": "json",
      "content": "[\n  {\n    \"id\": \"<UUID>\",\n    \"name\": \"docName\",\n    \"description\": \"desc\",\n    \"content\": \"filename or text\",\n    \"sessions\": [ \"<sessionId1>\", \"<sessionId2>\" ]\n  }\n]"
    }
  ]