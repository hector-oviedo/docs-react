[
    {
      "type": "header",
      "h": 3,
      "content": "Detach Document"
    },
    {
      "type": "p",
      "content": "Removes an attached document from a given session."
    },
    {
      "type": "header",
      "h": 5,
      "content": "METHOD: POST"
    },
    {
      "type": "snippet",
      "language": "bash",
      "content": "/documents/detach"
    },
    {
      "type": "header",
      "h": 3,
      "content": "Body"
    },
    {
      "type": "table",
      "headers": ["Field", "Type", "Required", "Description"],
      "rows": [
        ["doc_id", "string", "Yes", "ID of the document."],
        ["session_id", "string", "Yes", "Session to detach from."]
      ]
    },
    {
      "type": "header",
      "h": 3,
      "content": "Response"
    },
    {
      "type": "snippet",
      "language": "json",
      "content": "{\n  \"message\": \"Document detached\"\n}"
    }
  ]